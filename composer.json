            "Marv311\\BlueprintNovaAddon\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Marv311\\BlueprintNovaAddon\\BlueprintNovaAddonServiceProvider"
            ]
        }
    }
}
