{
    "name": "marv311/blueprint-nova-addon",
    "description": "",
    "keywords": [
        "marv311",
        "blueprint-nova-addon"
    ],
    "homepage": "https://github.com/marv311/blueprint-nova-addon",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Johnathon Marvell",
            "email": "marvell@weworkfreelance.com"
        }
    ],
    "require": {
        "illuminate/pipeline": "^6.0 || ^7.0 || ^8.0 || ^9.0 || ^10.0 || ^11.0 " ,
        "illuminate/support": "^6.0 || ^7.0 || ^8.0 || ^9.0 || ^10.0 || ^11.0" ,
        "laravel-shift/blueprint": "^1.18 || ^2.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0 || ^5.0 || ^6.0 || ^7.0 || ^8.0 || ^9.0",
        "mockery/mockery": "^1.3 || ^1.4",
        "phpunit/phpunit": "^9.0 || ^10.0 || ^11.0"
    },
    "autoload": {
        "psr-4": {
            "Marv311\\BlueprintNovaAddon\\": "src"
